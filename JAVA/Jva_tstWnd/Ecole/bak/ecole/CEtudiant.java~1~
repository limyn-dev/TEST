public class CEtudiant extends CPersonne
  {

  public CEtudiant()
    { super(); setSection(""); setUniversite("") }
  public CEtudiant(String nom, int anDebut, String section, String universite )
    { super(nom, anDebut); setSection(branche); setUniversite(universite)  }

  public void setSection(String section)        { Sct = section ;       }
  public void setUniversite(String universite)  { Uni = universite ;    }

  public String getSection()                    { return Sct ;      }
  public String getUniversite()                 { return Uni ;      }

  //---------------------------------------------------------------------------
  String Sct ;
  String Uni ;
  }