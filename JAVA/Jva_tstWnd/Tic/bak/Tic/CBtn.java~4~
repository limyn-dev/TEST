public class CBtn extends javax.swing.JButton
  {

  public CBtn(int lne, int col)
    {
    super() ;
    imClik = false ;
    }

  public void SetFlag()
    {
    flag=true ;
    }
  public boolean getFlag()
    {
    if (flag)
      {
      flag= false ;
      return true ;
      }
    else
      return false ;
    }

  private boolean flag ;

  }