public class CPersonne
  {

  public CPersonne()
    { setNom(""); setAnIn(0); }
  public CPersonne(String nom, int anDebut)
    { setNom(nom); setAnIn(anDebut); }

  public void setNom(String nom)    { Nom = nom ;   }
  public void setAnIn(int anDebut)  { AnIn = anDebut ; }

  public String getNom()          { return Nom ;  }
  public int    getAnIn()         { return AnIn ; }


  //---------------------------------------------------------------------------
  String Nom ;
  int    AnIn ;

  }