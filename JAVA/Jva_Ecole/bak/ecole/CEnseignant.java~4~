public abstract class CEnseignant extends CPersonel
  {

  public CEnseignant()
    { super(); setBranche(""); }
  public CEnseignant(String nom, int anDebut, String departement, int salaire,
                                                                 String branche)
    { super(nom, anDebut, departement, salaire); setBranche(branche) ;  }

  public void setBranche(String branche)  { Brc = branche ;   }

  public String getBranche()              { return Brc ;      }



  //---------------------------------------------------------------------------
  private String Brc ;
  }